---
let pages: PostPagenation;
({pages}=Astro.props)
---

<nav class="pagination" role="navigation" aria-label="pagination">
  {pages.prev.href !== ""
    ? <a class="pagination-previous" href={pages.prev.href}>&lt;&lt;</a>
    : <a class="pagination-previous" disabled>&lt;&lt;</a>
  }
  
  {pages.next.href !== ""
    ? <a class="pagination-next" href={pages.next.href}>&gt;&gt;</a>
    : <a class="pagination-next" disabled>&gt;&gt;</a>
  }

