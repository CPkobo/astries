---
import { classNaming }  from "$scripts/_className"
let blk: ListBlock<IsSingle>
({ blk } = Astro.props)


const presets = {
  def: "",
  circle: "list-circled",
};

const cls = classNaming("", blk.classes, blk.preset, presets)
---

<ul id={blk.id} class={cls}>
  {blk.$texts.map((t) => (
    <li>{t}</li>
  ))}
</ul>
