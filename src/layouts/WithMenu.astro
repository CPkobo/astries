---
import Astries from './Astries.astro';
import HeaderMenu from "$lib/Globals/Header/HeaderMenu.astro"
import FooterMenu from "$lib/Globals/Footer/FooterMenu.astro"
let head: AstriesHead;
let navigations: NavigationMenu[]
({ head, navigations } = Astro.props);
---

<Astries head={head}>
  <HeaderMenu navigations={navigations}/>
    <slot/>
  <FooterMenu navigations={navigations}/>
</Astries>
